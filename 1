IRrecv irrecv(irReceiverPin);
decode_results results;

void setup()
{
  Serial.begin(9600);

  irrecv.enableIRIn();
}

void loop()
{
  if(irrecv.decode(&results))
  {
    Serial.print("irCide: ");
    Serial.print(results.value, HEX);
    Serial.print(", bits: ");
    Serial.println(results.bits);
    irrecv.resume();
   }
}
